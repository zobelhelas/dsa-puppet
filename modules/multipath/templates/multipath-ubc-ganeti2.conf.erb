# conversion from MSA stype WWIDs to multipathd.conf:
#
#   sed -re 's#(.{6})(.{6})0000(.{2})(.*)#36\1000\2\3\4#'

blacklist {
        devnode "^(ram|raw|loop|fd|md|dm-|sr|scd|st)[0-9]*"
        devnode "^hd[a-z][[0-9]*]"
        devnode "^vd[a-z]"
        devnode "^cciss!c[0-9]d[0-9]*[p[0-9]*]"
        devnode "^sda[0-9]*$"
}
multipaths {
        # danzi
        multipath {
                wwid 3600c0ff00027786ca059e15701000000
                alias danzi
        }
        multipath {
                wwid 3600c0ff000277c5fa859e15701000000
                alias danzi-srv
        }

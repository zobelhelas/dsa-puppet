def $MUNIN_IPS = (<%=
begin
	v4ips.split(',').join(' ')
rescue
	''
end
%>);
def $MUNIN6_IPS = (<%=
begin
	v6ips == 'no' ? '' : v6ips.split(',').join(' ')
rescue
	''
end
%>);

domain ip {
        chain INPUT {
               daddr ($MUNIN_IPS) NOP;
        }
}

domain ip {
        chain OUTPUT {
               saddr ($MUNIN_IPS) NOP;
        }
}

domain ip6 {
        chain INPUT {
               daddr ($MUNIN6_IPS) NOP;
        }
}

domain ip6 {
        chain OUTPUT {
               saddr ($MUNIN6_IPS) NOP;
        }
}
